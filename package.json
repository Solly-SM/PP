{
  "name": "heartconnect-dating-app",
  "version": "1.0.0",
  "description": "A fully unique functional dating app with innovative matching features",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/app.js",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "build": "cd client && npm run build",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx"
  },
  "keywords": ["dating", "app", "matching", "social", "unique"],
  "author": "HeartConnect Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "dotenv": "^16.3.1",
    "validator": "^13.11.0",
    "nodemailer": "^6.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.4",
    "eslint": "^8.48.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}